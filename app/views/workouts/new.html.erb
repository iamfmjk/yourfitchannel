<div class="col-10 offset-1">
  <h1>Create new workout</h1>
  <%= simple_form_for @workout do |f| %>
    <%= f.input :title %>
    <%= f.input :video %>
    <%= f.input :scheduled_date, as: :date, start_year: Date.today.year, start_day: Date.today.day,          order: [:month, :day, :year] %>
    <%= f.input :scheduled_time %>
    <br />
    <%= f.submit 'Create', class: 'btn btn-primary' %>
  <% end %>
</div>
